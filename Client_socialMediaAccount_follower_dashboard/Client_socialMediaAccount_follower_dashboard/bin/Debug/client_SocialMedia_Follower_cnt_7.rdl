insert into client_social_media_follower_count
	(
		 				
		partyId					
		,displayName						
	)

select distinct	partyId
				,displayName 
from t1_faceBook





update client_social_media_follower_count
set faceBookAccount =o.onlineAddress 
    ,faceBook = b.cnt 
from client_social_media_follower_count a inner join  t1_facebook   b	on a.partyId = b.partyId
										  inner join  onlineaddress o	on a.partyId = o.partyId and o.onlineAddressTypeID = 4 

update client_social_media_follower_count
set twitterAccount =o.onlineAddress 
    ,twitter = b.cnt 
from client_social_media_follower_count a inner join  t2_Twitter    b	on a.partyId = b.partyId
										  inner join  onlineaddress o	on a.partyId = o.partyId  and o.onlineAddressTypeID = 5

update client_social_media_follower_count
set InstagramAccount =o.onlineAddress 
    ,instagram = b.cnt 
from client_social_media_follower_count a inner join  t3_instagram    b	on a.partyId = b.partyId
										  inner join  onlineaddress o	on a.partyId = o.partyId  and o.onlineAddressTypeID = 11




update client_social_media_follower_count
set youTubeAccount =o.onlineAddress 
    ,youTubeSubscribers = b.cnt 
from client_social_media_follower_count a inner join   t4_youTubeSubScribers    b	on a.partyId = b.partyId
										  inner join  onlineaddress o	on a.partyId = o.partyId  and o.onlineAddressTypeID = 17



update client_social_media_follower_count
set	youTubeViews = b.cnt 
from client_social_media_follower_count a inner join   t5_YouTubeViews 	   b	on a.partyId = b.partyId 
  inner join  onlineaddress o	on a.partyId = o.partyId  and o.onlineAddressTypeID = 17


select partyId	
	  ,displayName	
	 ,twitterAccount	
	 ,faceBookAccount	
	 ,InstagramAccount	
	 ,youTubeAccount	
	 ,FaceBook	
	 ,Twitter	
	 ,Instagram	
	 ,youTubeSubscribers	
	 ,youTubeViews
 from client_social_media_follower_count
 order by Instagram desc



