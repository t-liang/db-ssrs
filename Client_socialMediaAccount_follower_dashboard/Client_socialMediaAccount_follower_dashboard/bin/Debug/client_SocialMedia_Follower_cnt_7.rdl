insert into  t1_faceBook  
(
	PartyExternalIdId		
	,PartyId				 
	,ExternalId				
	,ExternalIdTypeId		
	,displayName			
	,ExternalIdType			
	,cnt					
)

select	a.PartyExternalIdId
		,a.PartyId	
		,a.ExternalId	
		,a.ExternalIdTypeId
		,c.displayName
		,ExternalIdType
		,max(a.NumDetail) as cnt 

		from partyexternalId a inner join partyRole b on a.partyId =b.partyid and b.partyRoleTypeId = 1
		inner join partyName c on c.partyId =a.partyid and partyFunctionTypeId &lt;&gt;7
		left join ExternalIdType d on a.ExternalIdTypeId= d.ExternalIdTypeId
where a.ExternalIdTypeId = 2
group by a.PartyExternalIdId
		,a.PartyId	
		,a.ExternalId	
		,a.ExternalIdTypeId
		,c.displayName
		,ExternalIdType



